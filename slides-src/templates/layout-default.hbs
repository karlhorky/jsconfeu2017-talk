<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 3.2 Final//EN">
<html>
<head>
  <title>{{slideNum}} of {{slideTotal}} | {{{presentationTitle}}}</title>
</head>
<body bgcolor="{{#if slide.bgcolor}}{{ slide.bgcolor }}{{else}}0d56c9{{/if}}" 
    link="#ffffff" alink="#ffffff" vlink="#ffffff">
<table border="0" cellpadding="0" cellspacing="0" width="100%">
    <tr>
        <td height={{#if fragment}}1{{else}}600{{/if}} valign=top>
            <table width=100% height=100% cellpadding=0 cellspacing=0>
                {{{ slideContent }}}
            </table>
        </td>
    </tr>
    {{#if withNavigation}}
    <tr valign="bottom">
        <td width="100%" cellpadding="0" cellspacing="0">
            <table width="100%" height="1">
                <tr>
                {{#if prevSlide}}
                    <td width="25%" align="left">
                        <a href="{{prevSlide}}" target=_top><font size="3">&lt; Prev</font></a>
                    </td>
                {{/if}}
                {{#if nextSlide}}
                    <td width="25%" align="right">
                        <a href="{{nextSlide}}" target=_top><font size="3">Next &gt;</font></a>
                    </td>
                {{/if}}
                </tr>
            </table>
        </td>
    </tr>
    {{/if}}
</table>
</body>
</html>