<table width=100% cellpadding=0 cellspacing=0 border=0 height=1>
    <tr valign="top">
        {{#if prevSlide}}
            <td width="25%" align="left">
                <a href="{{prevSlide}}" target=_top><font size="4">&lt; Prev</font></a>
            </td>
        {{/if}}
            <td width="25%" align="center">
                <a href="/slides/index" target=_top><font size="2">Index</font></a>
            </td>
        {{#if nextSlide}}
            <td width="25%" align="right">
                <a href="{{nextSlide}}" target=_top><font size="4">Next &gt;</font></a>
            </td>
        {{/if}}
    </tr>
</table>
